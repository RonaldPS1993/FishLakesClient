{
  "expo": {
    "name": "FishLakes",
    "slug": "FishLakes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/fishlakes.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ronaldpenasollet.FishLakes",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/fishlakes.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDXPFmxTjnvCTC60rZxjzlmUdCtazmb9zE"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/icons/fishlakes.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow FishLakes to use your location."
        }
      ],
      "expo-font"
    ],
    "newArchEnabled": true
  }
}
